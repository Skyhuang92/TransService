<Window x:Class="WCS.Trans.Password"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WCS.Trans"
        mc:Ignorable="d"
        Title="身份验证" Height="150" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Canvas>
        <TextBlock Text="密码：" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="16" Canvas.Left="16" Canvas.Top="30"/>
        <PasswordBox x:Name="txtPassword"  Canvas.Left="64" Canvas.Top="30" Width="203" Height="23" />
        <Button x:Name="btnOk" Content="确认" Canvas.Left="123" Canvas.Top="71" Width="54" Height="23" Click="btnOk_Click"/>
        <Button x:Name="btnCancle" Content="取消" Canvas.Left="203" Canvas.Top="71" Width="54" Height="23" Click="btnCancle_Click"/>
    </Canvas>
</Window>
