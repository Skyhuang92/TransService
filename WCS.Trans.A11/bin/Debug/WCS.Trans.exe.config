<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <!--多语言设置-->
    <add key="Language" value="zh_CN" />
    <!--OPC配置 OpcType = DA/UA-->
    <add key="OpcType" value="DA" />
    <add key="OpcServerName" value="Kepware.KEPServerEX.V6" />
    <add key="OpcServerHost" value="127.0.0.1" />
    <add key="OpcServerPort" value="0000" />
    <add key="OpcServerGroup" value="Trans" />
    <!--输送线区域-->
    <add key="LocArea" value="A11" />
    <add key="LocHost" value="127.0.0.1" />
    <!--数据库配置 DbType = Oracle\MsSql-->
    <add key="DbType" value="MsSql" />
    <add key="DbServerHost" value="127.0.0.1" />
    <add key="DbServerPort" value="1433" />
    <add key="DbServiceName" value="BTASRS" />
    <add key="DbUserId" value="sa" />
    <add key="DbPassword" value="#*c1234" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="BouncyCastle.Crypto" publicKeyToken="0e99375e54769942" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.8.3.0" newVersion="1.8.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>